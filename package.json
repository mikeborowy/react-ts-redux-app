    "react-addons-test-utils": "^15.6.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.6.2",
    "url": "^0.11.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.1",
