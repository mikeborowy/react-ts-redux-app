{
  "name": "react-ts-modularity-core",
  "version": "1.0.0",
  "description": "React with Typescript modularity (Core)",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev",
    "dev": "babel-node ./server/server.dev.js",
    "prod": "babel-node ./server/server.prod.js",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}' --fix",
    "build": "npm-run-all remove-ce clean-public test build-html build-wp",
    "build-wp": "babel-node ./scripts/build/build-wp.js",
    "build-html": "babel-node ./scripts/build/build-html.js",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "rimraf ./public",
    "remove-ce": "rimraf ./compile_errors"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --clearCache && jest"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "eslint --fix",
      "git add"
    ],
    "**/*.js?(x)": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "./configs/test/setup.js"
    ],
    "roots": [
      "./src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "axios": "^0.18.1",
    "cssnano": "^4.1.10",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-lazy-load-image-component": "^1.3.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.3",
    "@types/expect": "^1.20.4",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.3",
